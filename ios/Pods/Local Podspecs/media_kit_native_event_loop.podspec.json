{
  "name": "media_kit_native_event_loop",
  "version": "1.0.0",
  "summary": "Platform specific threaded event handling for media_kit.",
  "description": "Platform specific threaded event handling for media_kit.",
  "homepage": "https://github.com/media-kit/media-kit.git",
  "license": {
    "file": "../LICENSE"
  },
  "authors": {
    "Hitesh Kumar Saini": "saini123hitesh@gmail.com"
  },
  "source": {
    "path": "."
  },
  "dependencies": {
    "Flutter": []
  },
  "source_files": "Classes/**/*",
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "GCC_WARN_INHIBIT_ALL_WARNINGS": "YES",
    "HEADER_SEARCH_PATHS": "\"$(inherited)\" \"$(PROJECT_DIR)/../.symlinks/plugins/media_kit_native_event_loop/common/darwin/Headers\"",
    "FRAMEWORK_SEARCH_PATHS[sdk=iphoneos*]": "\"$(inherited)\" \"$(PROJECT_DIR)/../.symlinks/plugins/media_kit_libs_ios_video/ios/Frameworks/.symlinks/mpv/ios\"",
    "FRAMEWORK_SEARCH_PATHS[sdk=iphonesimulator*]": "\"$(inherited)\" \"$(PROJECT_DIR)/../.symlinks/plugins/media_kit_libs_ios_video/ios/Frameworks/.symlinks/mpv/ios-simulator\"",
    "OTHER_LDFLAGS": "\"$(inherited)\" -framework Mpv -lpthread",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "swift_version": "5.0"
}
